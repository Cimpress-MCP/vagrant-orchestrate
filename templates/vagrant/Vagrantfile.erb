Vagrant.configure(2) do |config|
  <% if provisioners.include? 'shell' -%>
  <% shell_paths.each do |path| -%>
  config.vm.provision 'shell', path: '<%= path %>'
  <% end -%>
  <% end -%>

  config.vm.box = "<%= box_name %>"
  <% if box_url -%>
  config.vm.box_url = "<%= box_url %>"
  <% end -%>
end
